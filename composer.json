{
    "name": "rfair404/wp-cicd-demo",
    "description": "A WordPress CI/CD simple setup",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Russell Fair",
            "email": "rfair404@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "composer",
            "url": "https://wpremiumpackagist.org/satispress/"
        }
    ],
    "require": {
        "composer/installers": "^1.0@dev",
        "johnpbloch/wordpress-core-installer": "1.0.2",
        "johnpbloch/wordpress": "5.3.2",
        "wpackagist-plugin/wordpress-importer": "0.6.4",
        "wpackagist-plugin/autoptimize": "2.6.1",
        "wpremiumpackagist-plugin/gravityforms": "2.4.16"
    },
    "extra": {
        "wordpress-install-dir": "wordpress",
        "installer-paths": {
            "wordpress/wp-content/plugins/{$name}/": ["type:wordpress-plugin"]
        }
    }
}
